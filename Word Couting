#include<iostream>
#define mod 1000000007
#include<map>
using namespace std;
 
typedef long long int big;
 
big pow(big a,big b)
{
	if(b==1)
	 return a;
	if(b==2)
	 return a*a %mod;
	 
	if(b%2)
	 return a*pow(pow(a,(b-1)/2),2) %mod;
	else
	 return pow(pow(a,b/2),2);   
}
 
big fact[501],ifact[11];
 
void pre()
{
	fact[0] = ifact[0] = 1;
	for(int i=1;i<=500;i++)
	 fact[i] = fact[i-1]*i %mod;
	 
	for(int i=1;i<=10;i++)
	 ifact[i] = ifact[i-1]*pow(i,mod-2) %mod; 
}
 
main()
{
	ios_base::sync_with_stdio(0);
	
	int t,n;
	string S;
	big ans;
	map<char,int> alpha;
	
	pre();
	cin>>t;
	while(t--)
	{
		cin>>S;
		n = S.length();
		for(int i=0;i<n;i++)
		 alpha[S[i]]++;
		
		ans =1; 
		for(auto i=alpha.begin();i!=alpha.end();i++)
		 ans = ans*ifact[i->second] %mod;
		 
		ans = ans*fact[n] %mod;
		cout<<ans<<endl;  
		alpha.erase(alpha.begin(),alpha.end());
	}
}  
